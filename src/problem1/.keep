const sum_to_n_a = function(n) {
    return (n * (n + 1)) / 2;
};

const sum_to_n_b = function(n) {
    let sum = 0;
    while (n > 0) {
        sum += n;
        n--;
    }
    return sum;
};

const sum_to_n_c = function(n) {
    return Array.from({ length: n }, (_, i) => i + 1)
                .reduce((prev, curr) => prev + curr, 0);
};
